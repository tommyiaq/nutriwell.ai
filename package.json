{
  "name": "nutrivell-ai",
  "version": "1.0.0",
  "description": "Smart AI-powered nutrition assistant platform",
  "private": true,
  "author": "NutriWell Team",
  "keywords": [
    "nutrition",
    "ai",
    "health",
    "diet",
    "wellness"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
